<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <Title>CSharp Helpers</Title>
    <Version>1.0.0</Version>
    <Copyright>Copyright 2019</Copyright>
    <Description>This library contains some useful extension methods for string.</Description>
    <Authors>Manoj Kulkarni</Authors>
    <Company>Manoj Kulkarni</Company>
    <PackageProjectUrl>https://github.com/manojkulkarni30/CSharpHelpers</PackageProjectUrl>
    <RepositoryUrl>https://github.com/manojkulkarni30/CSharpHelpers</RepositoryUrl>
    <PackageTags>c# string helpers</PackageTags>
    <PackageReleaseNotes>Migrated CSharpHelpers to .Net Standard</PackageReleaseNotes>
    <AssemblyVersion>1.0.0</AssemblyVersion>
    <FileVersion>1.0.0</FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageId>DotNetContext.Helpers</PackageId>
    <Product>DotNetContext.Helpers</Product>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
  </PropertyGroup>
</Project>